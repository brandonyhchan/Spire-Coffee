// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "file:./dev.db"
}

model User {
  id        Int    @id @default(autoincrement())
  userName  String @unique
  email     String @unique
  firstName String
  lastName  String
  password  String
}
